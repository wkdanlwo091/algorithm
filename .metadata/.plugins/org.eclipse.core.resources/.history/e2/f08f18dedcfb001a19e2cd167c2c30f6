package algorithm;

import java.util.Scanner;

public class BOJ_14889 {

	static int N;
	static int map[][];
	
	static void dfs(int cnt) {
		if(cnt == N/2) {
			
			return;
		}
		
		
		for(int i= 0;i< N;i++) {
			
		}
		
	}
	
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		
		N = scan.nextInt();
		map = new int[N][N];
		
		for(int i= 0 ;i < N;i++) {
			for(int j = 0;j< N;j++) {
				map[i][j] = scan.nextInt();
			}
		}
	}
}

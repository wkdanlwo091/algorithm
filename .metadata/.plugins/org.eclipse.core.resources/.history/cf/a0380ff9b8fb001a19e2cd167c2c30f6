package algorithm;

import java.util.Scanner;

public class BOJ_14888 {
	
	static int N;
	static int arr[];//숫자 값
	static int oper[];//+, -, * , /, 개수
	static int max;
	static int min;
	static int operator[];//숫자 사이에 들어가는 연산자
	static int visit[];//연산자 자리에 방문했나 체크
	
	static void dfs(int cnt) {
		
		if(cnt == N-1) {
			
			for(int i = 0;i< N-1;i++) {
				System.out.print(operator[i] + " ");
			}
			
		}
		
		for(int i= 0;i < N-1;i++) {
			if( visit[i] == 0) {
				
				int j = i;
				while(true) {
					if(oper[j%4] == 0) {
						j++;
					}else {
						visit[i] = 1;

						operator[cnt] = j%4;// 0 , 1, 2, 3
						oper[j%4]--;
						dfs(cnt+1);
						visit[i] = 0;

						break;
					}
				}

				
			}
		}
		
	}
	
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		N = scan.nextInt();
		arr = new int[N];
		oper = new int[4];
		
		operator = new int[N-1];
		visit = new int[N-1];
		
		
		for(int i = 0 ;i < N;i++) {
			arr[i] = scan.nextInt();
		}
		for(int i = 0;i < 4;i++) {
			oper[i] = scan.nextInt();
		}
		
	}
}

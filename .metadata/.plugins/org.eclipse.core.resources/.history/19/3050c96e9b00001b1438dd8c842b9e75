package algorithm;

import java.util.Scanner;

public class BOJ_1780 {
	static int N;
	static int map[][];
	static int a;
	static int b;
	static int c;
	
	static void dac(int y, int x, int size) {
		int none = 0;//negative one
		int zero = 0;
		int pone = 0;//plus one
		
		for(int i =y;i < y+ size;i++) {
			for(int j = x; j < x+ size; j++) {
				if(map[i][j] == 1) {
					pone++;
				}else if(map[i][j] == 0) {
					zero++;
				}else if(map[i][j] == -1) {
					none++;
				}
			}
		}
		
		if(none == size*size) {
			a++;
			return;
		}
		
		if(zero == size*size) {
			b++;
			return ;
		}
		
		if(pone == size * size) {
			c++;
			return;
		}
		
		for(int i = y; i < y+size;i++) {
			for(int j = x;j < x+size ; j++) {
				if(i % (size/3) == 1 && (j % (size/3) == 1)) {
					dac();
				}
			}
		}
		
	}
	
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		N = scan.nextInt();
		map = new int[N+1][N+1];
		for(int i= 1;i<=N;i++)
			for(int j = 1;j<= N;j++)
				map[i][j] = scan.nextInt();
		
		dac(1,1, N);
	}
}

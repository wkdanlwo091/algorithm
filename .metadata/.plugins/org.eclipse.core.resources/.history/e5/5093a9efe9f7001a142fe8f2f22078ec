package algorithm;

import java.util.Scanner;

public class BOJ_15651 {
	static int N;
	static int M;
	static int arr[];
	static int visit[];
	
	static void dp(int cnt) {
		if(cnt == M) {
			for(int i = 0 ;i < M;i++) {
				System.out.print(arr[i] + " ");
			}
			System.out.println();
			return;
		}
		
		for(int i = 0;i < N;i++) {
			if(!visit[i]) {
				visit[i] = true;
				arr[cnt] = i;
				dp(cnt+1);
				visit[i] = false;
			}
		}
		
	}
	
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		
		N = scan.nextInt();
		M = scan.nextInt();
		
		arr= new int[M+1];
		visit = new int[N+1];
		
		dp(0);
	}
	
}

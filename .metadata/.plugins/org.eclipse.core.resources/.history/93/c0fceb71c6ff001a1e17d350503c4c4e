package algorithm;

import java.util.Scanner;

public class BOJ_2580 {
	
	static int map[][];
	
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		map = new int[9][9];
		for(int i = 0 ;i < 9;i++) {
			for(int j =0 ;j < 9;j++) {
				map[i][j] = scan.nextInt();
			}
		}
	}
}

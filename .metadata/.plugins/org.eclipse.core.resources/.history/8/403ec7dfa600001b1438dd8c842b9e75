package algorithm;

import java.util.Scanner;

public class BOJ_10830 {

	static int N;
	static int[][] A;
	static int[][] result;
	static int B;
	
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		N = scan.nextInt();
		B = scan.nextInt();
		A = new int[N+1][N+1];
		result = new int[N+1][N+1];
		
		for(int i =1;i<= N;i++) {
			for(int j = 1;j <= N;j++) {
				A[i][j] = scan.nextInt();
			}
		}
		
		if(B == 1) {
			for(int i =1;i<= N;i++) {
				for(int j = 1;j <= N;j++) {
					System.out.print(A[i][j] + " ");
				}
				System.out.println();
			}
		}else {
			for(int x = 1;x < B;x++) {
				
				for(int i = 1;i<= N;i++) {
					for(int k = 1;k <= N;k++) {
						int sum = 0;
						for(int j = 1; j <= N;j++) {
							sum += A[i][j] * A[j][k];
						}
						result[i][k] = sum%1000;
					}
				}
				
			}
		}
		
		for(int i = 1;i<= N;i++) {
			for(int j = 1;j <= N;j++) {
				System.out.print();
			}
		}
		
	}
}

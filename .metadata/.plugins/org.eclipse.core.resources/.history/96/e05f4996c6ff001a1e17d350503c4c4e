package algorithm;

import java.util.Scanner;

public class BOJ_2580 {
	
	static int map[][];
	
	static void dfs() {
		
	}
	
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		map = new int[9][9];
		int y = 0, x = 0;
		for(int i = 0 ;i < 9;i++) {
			for(int j =0 ;j < 9;j++) {
				map[i][j] = scan.nextInt();
				if(map[i][j] == 0) {
					y = i;
					x = j;
				}
			}
		}
		
		dfs();
	}
}

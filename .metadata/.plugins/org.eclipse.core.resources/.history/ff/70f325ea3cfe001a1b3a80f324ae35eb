package algorithm;

import java.util.Scanner;

public class BOJ_2839 {

	static int dp(int N) {
		//3과 5
		if(N == 0) {
			return 0;
		}else {
			return -1;
		}
		
		return Math.min(dp(N-3)+1, dp(N-5)+1);	
	}
	
	public static void main(String[] args) {
		int N;
		Scanner scan = new Scanner(System.in);
		//dp , 배달하는 봉지를 최소 개수를 출력한다.
		N = scan.nextInt();
		
		dp(N);
	}
}

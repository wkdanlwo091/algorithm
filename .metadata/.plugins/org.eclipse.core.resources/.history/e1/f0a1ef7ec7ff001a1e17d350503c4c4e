package algorithm;

import java.util.Scanner;

public class BOJ_2580 {
	
	static int map[][];
	static int cnt = 0;
	
	static void dfs(int index, int y, int x) {
		if(index == cnt) {
			
			return;
		}
		
		//가로 검사
		
		for(int i = 0;i <x;i++) {
			
		}

		//세로 검사
		
		for(int i = 0;i <= y;i++) {
			
		}

		//9개 검사 
		
		
	}
	
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		map = new int[9][9];
		int y = -1, x = -1;
		for(int i = 0 ;i < 9;i++) {
			for(int j =0 ;j < 9;j++) {
				map[i][j] = scan.nextInt();
				if(map[i][j] == 0) {
					cnt++;
					if(y == -1) {
						y = i;
						x = j;

					}
				}
			}
		}
		
		dfs(0,y, x);
	}
}

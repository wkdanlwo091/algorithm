package algorithm;

import java.util.Scanner;

public class BOJ_2292 {
	
	public static void main(String[] args) {
		
		int N;
		Scanner scan = new Scanner(System.in);
		N = scan.nextInt();

		int num = 6;
		
		if(N == 1) {
			System.out.println("1");
			return;
		}
		
		for(int i = 1;i <=N;i++) {
			if(N <=num*i + 1) {
				System.out.println(num);
				break;
			}else {
				
			}
		}
	}
}

package algorithm;

public class Katalk_1 {

	
	
	public int solution(String s) {
		int answer = 0;
		
		int temp = 0;
		for(int i =1 ;i< s.length()/2;i++) {// i개로 자르기 
			
			temp = 0;
			for(int j = 0 ;j < s.length();j++) {//j 기준으로 비교하기

				for(int k = j+i ; k < s.length(); k++) {
					
					if(k+i <=s.length()) {//비교대상이 index안에 있을 때
						if(s.substring(j, j+i).equals(s.substring(k, k+i))) {
							
							//i= 1이고 aa면    i= 2 고 aaaa면 
							temp = temp + i;// temp = temp + 1; temp = temp +2;
							
							if() {
								
							}
							
						}else {//aab일때 기준점을 b로 잡는다. 
							
						}
						
					}else {//index가 벗어났을 때
						
						
					}
				}
			}
		}
		
		
	    return answer;
	}

	public static void main(String[] args) {
		Katalk_1 a  =new Katalk_1();
		System.out.println(a.solution("aabbaccc"));
	}
}

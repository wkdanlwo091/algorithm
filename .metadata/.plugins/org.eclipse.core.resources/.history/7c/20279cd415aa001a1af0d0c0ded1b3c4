package algorithm;

import java.util.Arrays;
import java.util.Scanner;

public class BOJ_11399 {

	public static void main(String[] args) {
		int N;
		int P[];
		int answer = 0;

		Scanner scan = new Scanner(System.in);
		
		N = scan.nextInt();
		P = new int[N];
		
		for(int i = 0;i < P.length;i++) {
			P[i] = scan.nextInt();
		}
		
		for(int i = 0 ;i<  P.length;i++) {
			answer = answer +  P[i];
		}
		Arrays.parallelSort(P);
	}

}

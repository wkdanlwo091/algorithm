package algorithm;

import java.util.Scanner;

public class BOJ_5622 {

	public static void main(String[] args) {
		String st;
		int ans = 0;
		int temp = 0;
		Scanner scan = new Scanner(System.in);
		st = scan.next();
		
		for(int i = 0;i <st.length();i++) {
			temp = (int)(st.charAt(i)-'A')/3 + 3;
		}
		
	}
}

package algorithm;

import java.util.Scanner;

public class BOJ_11720 {
	static int N;


	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		String st;
		N = scan.nextInt();
		st = scan.nextLine();
		int sum = 0;
		for(int i = 0 ;i< N;i++) {
			sum += Integer.parseInt(st.charAt(i));
			System.out.println(sum);
			
		}
		System.out.println(sum);
	}
}

package algorithm;

import java.util.Scanner;

public class BOJ_1541 {

	
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		String a;
		
		a = scan.next();
		
		int sum = 0;
		
		for(int i = 0 ;i< a.length();i++) {
			
			if(a.charAt(i) == '+') {
				
			}else if(a.charAt(i) == '-') {
				int start = i+1;
				int end;
				int j = i+1;
				while(true) {
					
					if(a.charAt(j) == '+') {
						
						sum -= Integer.parseInt(a.substring(start, j));//-·Î •û±â
						end = j-1;
						
						
						start = j+1;
					}
					j++;
				}
			}
				
		}
		
	}
}
